-- Black screen overlay for 2 seconds
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")

ScreenGui.IgnoreGuiInset = true
ScreenGui.ResetOnSpawn = false
ScreenGui.Name = "BlackoutOverlay"
ScreenGui.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")

Frame.Size = UDim2.new(1, 0, 1, 0)
Frame.BackgroundColor3 = Color3.new(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Parent = ScreenGui

task.delay(2, function()
    ScreenGui:Destroy()
end)

-- Load Cream Glide
loadstring(game:HttpGet("https://raw.githubusercontent.com/LeeGitter/td-scripts/refs/heads/main/Cream%20Glide", true))()

-- Load Cream to Oreo
loadstring(game:HttpGet("https://raw.githubusercontent.com/LeeGitter/td-scripts/refs/heads/main/Cream%20to%20Oreo", true))()
